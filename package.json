{
	"name": "deviz",
	"displayName": "Deviz",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:deviz.startSession"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "deviz.startSession",
				"title": "Deviz: Start session"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "devizPanes",
					"name": "Deviz"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "devizPanes",
